{
  "name": "rmtram/array2tables",
  "type": "library",
  "description": "Array to Markdown table, Html table, Ascii table",
  "keywords": [
    "markdown",
    "html",
    "ascii",
    "table",
    "array"
  ],
  "homepage": "https://github.com/Rmtram/Array2Tables",
  "support": {
    "issues": "https://github.com/Rmtram/Array2Tables/issues",
    "source": "https://github.com/Rmtram/Array2Tables"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit",
    "test:reset": "./vendor/bin/phpunit -d --update-snapshots",
    "lint": "./vendor/bin/php-cs-fixer fix --dry-run --diff -v",
    "lint:fix": "./vendor/bin/php-cs-fixer fix --diff -v"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Rmtram",
      "email": "rmtram95@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "rmtram/sorter": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~8.0",
    "friendsofphp/php-cs-fixer": "^2.16",
    "spatie/phpunit-snapshot-assertions": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Rmtram\\Array2Tables\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ],
    "psr-4": {
      "Rmtram\\Array2Tables\\TestCase\\": "tests/"
    }
  }
}
